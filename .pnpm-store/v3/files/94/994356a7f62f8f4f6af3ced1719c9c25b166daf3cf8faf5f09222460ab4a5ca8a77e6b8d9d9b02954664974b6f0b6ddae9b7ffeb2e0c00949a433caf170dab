import { PluginConfig } from 'ttypescript/lib/PluginCreator';
import ts from 'typescript';
export interface TsTransformExtensionsConfig extends PluginConfig {
    extensions?: {
        [key: string]: string;
    };
}
export default function transformer(program: ts.Program, config: TsTransformExtensionsConfig): (context: ts.TransformationContext) => (sourceFile: ts.SourceFile) => ts.SourceFile;
