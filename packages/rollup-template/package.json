{
  "name": "@aiou/rollup-template",
  "version": "1.1.0",
  "main": "lib/how-long-till-lunch.cjs.js",
  "module": "lib/how-long-till-lunch.esm.js",
  "browser": "lib/how-long-till-lunch.umd.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "ms": "2.1.1"
  },
  "devDependencies": {
    "@types/ms": "0.7.30",
    "husky": "3.0.1",
    "np": "5.0.3",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "rimraf": "3.0.0",
    "rollup": "1.12.2",
    "rollup-plugin-alias": "^2.0.0",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-typescript2": "0.24.0",
    "ts-node": "8.1.0",
    "tslib": "1.9.3",
    "typescript": "3.4.5"
  },
  "scripts": {
    "build": "rimraf lib && rollup -c",
    "dev": "rollup -c -w",
    "test": "ts-node test/test.ts",
    "pretest": "npm run build",
    "update": "npm update && npm update --save-dev",
    "check": "npm outdated && npm outdated --save-dev",
    "prepublishOnly": "np --no-cleanup --yolo --no-publish --any-branch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
