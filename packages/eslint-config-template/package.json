{
  "name": "@aiou/eslint-config-template",
  "version": "0.1.4",
  "description": "eslint config template for aiou template",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/JiangWeixian/templates#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/JiangWeixian/templates",
    "directory": "packages/eslint-config-template"
  },
  "bugs": {
    "url": "https://github.com/JiangWeixian/templates/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": "JW <jiangweixian1994@gmail.com> (https://twitter.com/jiangweixian)",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --config=index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/**/*.{js,ts,vue,json}": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": ">=7.30.0",
    "prettier": ">=2.3.2",
    "pretty-quick": ">=3.1.1"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsonc": "^1.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "eslint-plugin-yml": "^0.9.0",
    "jsonc-eslint-parser": "^1.1.0",
    "yaml-eslint-parser": "^0.3.2"
  },
  "devDependencies": {
    "cz-emoji": "^1.3.1",
    "eslint": "^7.22.0",
    "lint-staged": "^11.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
