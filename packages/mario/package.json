    "build": "rimraf lib && rollup -c --silent && pnpm run build:dts",
    "dev": "rimraf lib && rollup -c --watch",
    "vitest": "vitest",
    "vitest:ci": "NO_COLOR=1 vitest",
    "test": "pnpm run build && pnpm run vitest:ci",
    "pretest": "pnpm run build",
    "lint:fix": "eslint . --fix"
  },
