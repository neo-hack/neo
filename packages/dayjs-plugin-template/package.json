{
  "name": "@aiou/dayjs-plugin-template",
  "version": "0.1.1",
  "description": "dayjs plugin template",
  "main": "lib/index.js",
  "typings": "src/typings/index.d.ts",
  "repository": {
    "url": "https://github.com/JiangWeixian/templates/tree/master/packages/ts-lib-template"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "dayjs",
    "dayjs-plugin",
    "template",
    "aiou"
  ],
  "watch": {
    "run_dev": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "quiet": true,
      "legacyWatch": true,
      "delay": 1000,
      "runOnChangeOnly": false
    }
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.0.12",
    "@types/jest": "24.0.18",
    "@types/lodash.chunk": "^4.2.6",
    "copyfiles": "^2.4.1",
    "dayjs": "^1.9.7",
    "eslint": "^7.25.0",
    "husky": "3.0.1",
    "jest": "24.9.0",
    "np": "5.0.3",
    "npm-watch": "0.6.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "3.0.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "^2.2.3"
  },
  "peerDependencies": {
    "dayjs": "*"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "author": "JW",
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib && ttsc",
    "run_dev": "rimraf lib && ttsc",
    "dev": "npm-watch run_dev",
    "test": "jest",
    "update": "npm update && npm update --save-dev",
    "check": "npm outdated && npm outdated --save-dev",
    "release": "npm run build && np --no-cleanup --yolo --no-publish --any-branch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
