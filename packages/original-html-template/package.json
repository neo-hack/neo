    "@types/webpack": "4.4.31",
    "@types/webpack-bundle-analyzer": "2.13.1",
    "@types/webpack-dev-server": "3.9.0",
    "@types/webpack-env": "1.13.9",
    "@types/webpack-merge": "4.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.5",
