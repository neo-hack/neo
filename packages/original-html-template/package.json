{
  "name": "@aiou/original-html-template",
  "version": "1.3.1",
  "description": "original html template",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js --hot --progress --host 0.0.0.0",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.config.js --progress",
    "update": "npm update && npm update --save-dev",
    "check": "npm outdated && npm outdated --save-dev",
    "prepublishOnly": "np --no-cleanup --yolo --no-publish --any-branch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiangWeixian/templates.git"
  },
  "author": "jiangwei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JiangWeixian/templates/issues"
  },
  "homepage": "https://github.com/JiangWeixian/templates#readme",
  "husky": {
    "hooks": {
      "pre-commit": "prettier --config .prettierrc --loglevel warn --write '**/*' && stylus-supremacy format 'src/**/*.styl' -r -p .stylintrc"
    }
  },
  "dependencies": {
    "animejs": "3.1.0",
    "document-ready": "2.0.2"
  },
  "devDependencies": {
